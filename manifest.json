{
    "manifest_version": 2,
    "name": "notebook scrolling enabling",
    "version": "0.1",
     "browser_action": {
     },
    "content_scripts": [
      {
        "matches": [
          "*://nbviewer.jupyter.org/*.ipynb",
          "*://github.com/*.ipynb"
        ],
        "js": ["main.js"]
      }
    ]
  }